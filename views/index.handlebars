<div class="container">
	<div class="row">
		<div class="col s6 m6 offset-s3 offset-m3 center">
			<h1>Synthtopia: The Return</h1>
		</div>
	</div>
	<div class="row">
		{{#each dbPost}}
		<div class="col s12 m8">
			<div class="card">
				<div class="card-content">
					<span class="card-title">{{this.title}}</span>
					<img class="materialboxed" width="650" src="{{this.photo}}">
					<a data-link={{this.link}} class="waves-effect waves-light btn devourBtn"><i class="material-icons left">more_horiz</i>More</a>
				</div>
			</div>
		</div>
		{{/each}}
	</div>	
</div>